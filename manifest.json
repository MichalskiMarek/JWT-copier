{
  "manifest_version": 2,
  "name": "JWT copier",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": {
      "16": "icon16.png"
    }
  },
  "permissions": ["clipboardWrite"]
}
